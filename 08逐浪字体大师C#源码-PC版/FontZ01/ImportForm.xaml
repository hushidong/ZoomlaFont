<Window x:Class="FontZ01.ImportForm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FontZ01"
        mc:Ignorable="d"
        Title="批注上载" Height="228" Width="611"  WindowStartupLocation="CenterScreen">
    <Grid Background="#FFE2DEDE">
        <Button x:Name="BtnBrow" Content="选择(可多选)" HorizontalAlignment="Left" Margin="175,32,0,0" VerticalAlignment="Top" Width="104" Height="36" Click="BtnBrow_Click"/>
        <Button x:Name="BtnUpdateDisplayName" Content="更新表列批注" HorizontalAlignment="Left" Margin="330,32,0,0" VerticalAlignment="Top" Width="104" Height="36" Click="BtnUpdateDisplayName_Click"/>
        <GroupBox x:Name="GP_Import" Header="批注数据导入" HorizontalAlignment="Left" Height="102" Margin="10,76,0,0" VerticalAlignment="Top" Width="583">
            <Label x:Name="LablePath" Content="文件路径：" HorizontalAlignment="Left" Margin="0,20,0,0" VerticalAlignment="Top"/>
        </GroupBox>
        <TextBox x:Name="txtMulItem" HorizontalAlignment="Left" Height="72" Margin="103,93,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="452"/>

    </Grid>
</Window>
